log(phone)\n        console.log(address)\n        console.log(pincode)\n        console.log(apt)\n        console.log(city)\n        console.log(state)\n        console.log(totalitem)\n      }, [])\n    \n      const orderButton = (e) => {\n        let params = {\n          total_price: subtotal,\n          full_name: full_name,\n          address_line1: address,\n          address_line2: apt,\n          city: city,\n          state: state,\n          postal_code: pincode,\n          country: \"US\",\n          telephone: phone,\n        };\n        api.orderAdd(params).then(window.location.replace(\"thankyou\"));\n        e.preventDefault();\n      };\n\n    return (\n        <>\n        <section class=\"main3\">\n          <h2>------Order Your Items------</h2> \n          <br/><br/>\n        </section>\n      <section class=\"bg-checkout-data\">\n        <div class=\"product-details\">\n          <p class=\"shipment-details\">Shipment Details</p>\n          <p class=\"confirm\">Please check your items and confirm it</p>\n          <table>\n            {carts &&\n              carts.map((cart) => (\n                <tr>\n                  <td class=\"td-item\">{cart.item.name}</td>\n                  <td class=\"td-quantity\">{cart.quantity}</td>\n                  <td class=\"td-price\">$ {cart.item.price}</td>\n                </tr>\n              ))}\n            <tr class=\"total\">\n              <td class=\"td-item\">Total</td>\n              <td class=\"td-quantity\">{totalitem}</td>\n              <td class=\"td-price\">$ {subtotal}</td>\n            </tr>\n          </table>\n        </div>\n        <div class=\"address-details\">\n          <form action=\"/\">\n            <label for=\"name\">Full name</label>\n            <input\n              type=\"text\"\n              name=\"name\"\n              id=\"name\"\n              placeholder=\"Enter Recipients name\"\n              onChange={inputFullname}\n              required\n            />\n\n            <label for=\"phone\">Phone Number</label>\n            <input\n              type=\"text\"\n              name=\"phone\"\n              id=\"name\"\n              placeholder=\"Enter Phone Number\"\n              onChange={inputPhoneNumber}\n              required\n            />\n\n            <label for=\"address\">Street address or P.O. Box</label>\n            <input\n              type=\"text\"\n              name=\"address\"\n              id=\"name\"\n              placeholder=\"Enter Street address or P.O. Box\"\n              onChange={inputAddress}\n              required\n            />\n\n            <label for=\"pin\">PIN Code</label>\n            <input\n              type=\"text\"\n              name=\"pin\"\n              id=\"name\"\n              placeholder=\"Enter PIN Code\"\n              onChange={inputPin}\n              required\n            />\n\n            <label for=\"apt\">Apt, suit, unit, building, floor, etc</label>\n            <input\n              type=\"text\"\n              name=\"apt\"\n              id=\"name\"\n              placeholder=\"Enter Apt, suit, unit, building, floor, etc\"\n              onChange={inputApt}\n              required\n            />\n\n            <label for=\"city\">City</label>\n            <input\n              type=\"text\"\n              name=\"city\"\n              id=\"name\"\n              placeholder=\"Enter City\"\n              onChange={inputCity}\n              required\n            />\n\n            <label for=\"state\">State</label>\n            <input\n              type=\"text\"\n              name=\"state\"\n              id=\"name\"\n              placeholder=\"Enter State\"\n              onChange={inputState}\n              required\n            />\n\n            <input\n              type=\"submit\"\n              name=\"submit\"\n              value=\"SUBMIT\"\n              class=\"checkout-btn\"\n              onClick={orderButton}\n            />\n          </form>\n       